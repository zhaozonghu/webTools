.loading .spinner > div{display: inline-block; width: .2rem; height: .2rem; -webkit-animation: bouncedelay 1.4s infinite ease-in-out; animation: bouncedelay 1.4s infinite ease-in-out; border-radius: 100%; background-color: #999; -webkit-animation-fill-mode: both; animation-fill-mode: both}
.loading .spinner .bounce1{margin-right: .09rem; -webkit-animation-delay: -.32s; animation-delay: -.32s}
.loading .spinner .bounce2{margin-right: .09rem; -webkit-animation-delay: -.16s; animation-delay: -.16s}
@-webkit-keyframes bouncedelay{0%, 100%, 80%{-webkit-transform: scale(0)} 40%{-webkit-transform: scale(1)}}
@keyframes bouncedelay{0%, 100%, 80%{-webkit-transform: scale(0); transform: scale(0)} 40%{-webkit-transform: scale(1); transform: scale(1)}}

/*--上拉刷新svg动画--*/
.svg{position: absolute;z-index: 999;top: 1.4rem; left: 50%; display: block; width: 80px; height: 80px; margin-left: -20px; opacity: 0; border-radius: 50%; background-color: #fff;}
.read-position, .recommend-news{text-align: center; vertical-align: middle; color: #2a90d7; background: rgba(213, 233, 247, .9)}
.svg.active{-webkit-animation: myrotate 1s infinite ease-in-out; animation: myrotate 1s infinite ease-in-out}
@-webkit-keyframes myrotate{0%{-webkit-transform: rotate(0deg); transform: rotate(0deg)} 100%{-webkit-transform: rotate(360deg); transform: rotate(360deg)}}
@keyframes myrotate{0%{-webkit-transform: rotate(0deg); transform: rotate(0deg)} 100%{-webkit-transform: rotate(360deg); transform: rotate(360deg)}}

/*--刷新动画--*/
@-webkit-keyframes x-spin{0%{-webkit-transform: rotate(0deg)} 50%{-webkit-transform: rotate(180deg)} to{-webkit-transform: rotate(1turn)}}

/*--幻灯--*/
.swiper-container{margin: 0 15px; padding-top: 15px}
[data-dpr="2"] .swiper-container{margin: 0 30px; padding-top: 30px}
[data-dpr="3"] .swiper-container{margin: 0 45px; padding-top: 45px}
.swiper-slide{text-align: center; font-size: 18px; background: #fff; display: -webkit-box; display: -ms-flexbox; display: -webkit-flex; display: flex; -webkit-box-pack: center; -ms-flex-pack: center; -webkit-justify-content: center; justify-content: center; -webkit-box-align: center; -ms-flex-align: center; -webkit-align-items: center; align-items: center;}
.swiper-slide a, .swiper-slide img{width: 100%}

/*--下载提示层--*/
.app_downPanel .com_pannel{position: fixed; width: 100%; height: 50px; background: rgba(0, 0, 0, .7); background-repeat: no-repeat; background-position: bottom; background-size: 100% 100%; color: #fff;z-index: 5000;}
[data-dpr="1"] .app_downPanel .com_pannel{max-width: 750px}
[data-dpr="2"] .app_downPanel .com_pannel{height: 100px}
[data-dpr="3"] .app_downPanel .com_pannel{height: 150px}

.app_downPanel .pannel_top{top: 0}
.app_downPanel .pannel_bottom{bottom: 0}
.app_downPanel .show-top-pannel{-webkit-transform: translateY(0); transform: translateY(0); -webkit-transition: all .5s cubic-bezier(.19, 1, .22, 1); transition: all .5s cubic-bezier(.19, 1, .22, 1)}
.app_downPanel .hide-top-pannel{-webkit-transform: translateY(-100%); transform: translateY(-100%); -webkit-transition: all .3s cubic-bezier(.55, .055, .675, .19); transition: all .3s cubic-bezier(.55, .055, .675, .19)}
.app_downPanel .banner-label, .app_downPanel .close, .app_downPanel .logo, .app_downPanel .open{position: absolute; top: 50%; -webkit-transform: translateY(-50%); transform: translateY(-50%)}
.app_downPanel .logo{height: 30px; width: 30px; left: 15px; background:#fff url(../images/logo.png) no-repeat; border-radius: 6px; overflow: hidden; background-size: 30px}
[data-dpr="2"] .app_downPanel .logo{width: 60px;height: 60px;left: 30px;border-radius: 12px;background-size: 60px;}
[data-dpr="3"] .app_downPanel .logo{width: 90px;height: 90px;left:45px;border-radius: 18px;background-size: 90px;}

.app_downPanel .banner-label{left: 50px}
[data-dpr="2"] .app_downPanel .banner-label{left: 100px}
[data-dpr="3"] .app_downPanel .banner-label{left: 150px}

.app_downPanel .banner-label p{color: #fff; font-size: 12px; line-height: 15px}
[data-dpr="2"] .app_downPanel .banner-label p{font-size: 24px;line-height: 30px}
[data-dpr="3"] .app_downPanel .banner-label p{font-size: 36px;line-height: 45px}

.app_downPanel .banner-label .tb{font-size: 15px; padding-bottom: 3px}
[data-dpr="2"] .app_downPanel .banner-label .tb{font-size: 30px;padding-bottom: 6px}
[data-dpr="3"] .app_downPanel .banner-label .tb{font-size: 45px;padding-bottom: 9px}

.app_downPanel .close{height: 40px; width: 20px; padding: 5px; position: absolute; right: 0; box-sizing: content-box; background: url(../images/ico_close.png) no-repeat 50%; background-size: 14px; top: 50%; -webkit-transform: translateY(-50%); transform: translateY(-50%)}
[data-dpr="2"] .app_downPanel .close{height: 80px;width: 40px;padding: 10px;background-size: 28px}
[data-dpr="3"] .app_downPanel .close{height: 120px;width: 60px;padding: 15px;background-size: 42px}

.app_downPanel .open{right: 40px; width: 60px; font-size: 13px; line-height: 30px; padding: 0 10px; border-radius: 6px; background: #fd7f04; display: inline-block; color: #fff; box-sizing: content-box; text-align: center}
[data-dpr="2"] .app_downPanel .open{right: 80px;width: 120px;font-size: 26px;line-height: 60px;padding: 0 20px;border-radius: 12px}
[data-dpr="3"] .app_downPanel .open{right: 120px;width: 180px;font-size: 39px;line-height: 90px;padding: 0 30px;border-radius: 18px}

/*--分享--*/
.share_wrapper{width: 100%;text-align: center;padding: .3rem 0 0 0;}
.share_wrapper a{width:1.2rem;height:1.2rem;line-height:1.2rem;border-radius: 100%;margin: 0 5px;background: #000;display: inline-block;text-align: center;}
.share_wrapper .share_movement{background: url(../images/share/weixin.png) no-repeat;background-size:100%; }
.share_wrapper .share_wechat{background: url(../images/share/weixin_friend.png) no-repeat;background-size:100%; }
.share_wrapper .share_sina{background: url(../images/share/weibo.png) no-repeat;background-size:100%; }
.share_wrapper .share_qzone{background: url(../images/share/qqzone.png) no-repeat;background-size:100%; }
.share_wrapper .share_qq{background: url(../images/share/qq.png) no-repeat;background-size:100%; }
