.up-letv {
    position: fixed;
    right: 8px;
    width: 4.2rem;
    display: none;
    z-index: 100;
    bottom: 65px
}

.up-letv span {
    position: relative;
    margin-top: 5px
}

div.up-letv i, div.up-letv b, div.up-letv a {
    width: 4.2rem;
    height: 4.2rem;
    cursor: pointer;
    display: block
}

.up-letv.isgoTop {
    display: block
}

.ani_goTopShow {
    -webkit-animation: ani_goTopShow 0.2s ease-out 1;
    -webkit-animation-fill-mode: forwards
}

@-webkit-keyframes ani_goTopShow {
    0% {
        bottom: -100px
    }
    100% {
        bottom: 65px
    }
}

.ani_goTopHide {
    -webkit-animation: ani_goTopHide 0.2s ease-out 1;
    -webkit-animation-fill-mode: forwards
}

@-webkit-keyframes ani_goTopHide {
    0% {
        bottom: 65px
    }
    100% {
        bottom: -100px
    }
}

.up-letv .s-bg {
    background-color: #000;
