@charset "utf-8";
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td {
    margin: 0;
    padding: 0
}

body, button, input, select, textarea {
    font: 12px/1.5 "\5FAE\8F6F\96C5\9ED1", arial;
    *line-height: 1.5
}

h1, h2, h3, h4, h5, h6 {
    font-size: 100%;
    font-weight: normal
}

address, cite, dfn, em, var {
    font-style: normal
}

code, kbd, pre, samp {
    font-family: courier new, courier, monospace
}

ul, ol {
    list-style: none
}

fieldset, img {
    border: 0
}

table {
    border-collapse: collapse;
    border-spacing: 0
}

input, button, textarea, select, optgroup, option {
    font-family: inherit;
    font-size: inherit;
    font-style: inherit;
    font-weight: inherit;
    font-size: 100%
}

div:after, ul:after, dl:after, .clearfix:after {
    content: "";
    display: block;
    clear: both;
    height: 0;
    visibility: hidden
}

abbr, article, aside, audio, canvas, datalist, details, figure, footer, header, hgroup, menu, nav, output, progress, section, video {
    display: block;
    margin: 0;
    padding: 0
}

i, u, b, em, span, small {
    font-weight: normal;
    font-style: normal
}

a {
    text-decoration: none;
    color: #393939
}

a:visited {
    color: #393939;
    text-decoration: none
}

a:hover {
    color: #393939;
    text-decoration: none
}

.clearfix {
    clear: both;
    zoom: 1
}

html, body {
    height: 100%
}

html {
    font-size: 62.5%
}

body {
    font-size: 1.2rem;
    color: #1a1a1a;
    background: #f7f7f7;
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
    -webkit-user-select: none;
    user-select: none;
    -moz-user-focus: none;
    -moz-user-select: none
}

h2 {
    margin: 0;
    padding: 0
}

ul {
    letter-spacing: -4px;
    word-spacing: -4px;
    font-size: 0
}

ul li {
    font-size: 1.2rem;
    letter-spacing: 0;
    word-spacing: 0
}

ul.boxFlex {
    width: 100%;
    display: box;
    display: -webkit-box;
    display: -moz-box
}

ul.boxFlex li {
    float: left;
    position: relative;
    box-flex: 1;
    -webkit-box-flex: 1;
    -moz-box-flex: 1
}

section {
    margin: 0 auto;
    height: auto;
    overflow: hidden;
    min-width: 310px;
    position: relative
}

section .column_body {
    position: relative
}

body, html {
    background: #fff
}

.f-hide {
    display: none
}

.f-fl {
    float: left
}

.f-fr {
    float: right
}

header {
    position: relative;
    height: 4.4rem;
    overflow: hidden
}

header .headBox {
    position: fixed;
    width: 100%;
    z-index: 101;
    top: 0;
    left: 0;
    background: #ffffff;
    border-bottom: 1px solid #dedfe0
}

header.focus {
    overflow: initial
}

header.focus .headBox {
    position: absolute
}

header .logo {
    float: left;
    padding-left: 1rem;
    overflow: hidden
}

header .icon_channal {
    float: left;
    position: relative;
    top: 0.4rem;
    margin-left: -1.5rem;
    font-size: 2rem;
    font-weight: bold;
    line-height: 4rem
}

header .userBox {
    float: right;
    height: 4.4rem;
    overflow: hidden
}

header .userBox .hover {
    color: #d70c19
}

.icon_user_img:before {
    content: ""
}

.icon_user_in img {
    width: 2rem;
    height: 2rem;
    border-radius: 50%;
    display: inline-block;
    position: relative;
    top: 1.5px
}

header .userBox span, header .userBox a {
    display: inline-block;
    width: 4.3rem;
    height: 4.4rem;
    line-height: 4.4rem;
    text-align: center
}

nav.pd_nav {
    position: fixed;
    top: 4.4rem;
    z-index: 101;
    width: 100%;
    background: rgba(0, 0, 0, 0.85)
}

nav.pd_nav li a {
    display: block;
    width: 100%;
    height: 100%;
    color: #fff
}

nav li {
    float: left;
    text-align: center;
    font-size: 13px
}

nav li a {
    display: block;
    width: 100%;
    height: 100%;
    color: #fff
}

nav .channel_nav li {
    width: 19.6%;
    line-height: 36px
}

nav.pd_nav .channel_nav {
    padding: 0
}

nav li {
    width: 19.6%;
    margin-top: 4px;
    margin-bottom: 4px
}

nav .channel_nav li a:hover {
    text-decoration: none
}

nav .shengtaiNav {
    color: #ea6250;
    font-size: 16px;
    margin: 4px 1.6rem 4px 1.6rem
}

@media (orientation: landscape) {
    .pd_nav .channel_nav li {
        width: 14%
    }

    nav .shengtaiNav {
        color: #ea6250
    }
}

