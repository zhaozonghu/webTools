var u = navigator.userAgent.toLowerCase();
$(function () {
    if(checkQyingBrowser()){
        $('.down-way').attr('href',qyVideoHref);
        return false;
    }
    if((u.indexOf('android') > -1 || u.indexOf('linux') > -1)){
        //安卓地址跳转
        $('.down-way').attr('href','http://center.iqying.com/app_root/qy_1212_bd.apk');//安卓跳转地址
    }else if(u.indexOf('iphone') > -1 || u.indexOf('ipad') > -1) {
        //IOS地址跳转
        $('.down-way').attr('href','https://at.umeng.com/4ziuqu');//ios跳转
    }else{
        $('.down-way').attr('href',''); //未检测到
    }
});
function checkQyingBrowser(){
    if(u.indexOf("qybrowser") != -1){
        return true;
    }
    return false;
};